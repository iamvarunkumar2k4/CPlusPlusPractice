#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main()
{
    int x,y,s,i,num;
    cin>>x>>y;
    int *arr[x];
    while(x--)
    {
        int num;
        cin>>num;
        arr[s]=new int[num];
        for(i=0;i<num;i++)
        {
            cin>>arr[s][i];
        }
        s++;
    }
    while(y--)
    {
        int a,b;
        cin>>a;
        cin>>b;
        cout<<arr[a][b]<<endl;
    }
}
